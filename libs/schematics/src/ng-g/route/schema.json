{
  "$schema": "http://json-schema.org/schema",
  "$id": "SchematicsNtersolRoute",
  "title": "Ntersol Route Options Schema",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Route name",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    },
    "path": {
      "type": "string",
      "format": "path",
      "description": "The path to create the route",
      "visible": false
    }
  },
  "required": ["name"]
}
